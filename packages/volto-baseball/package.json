{
  "name": "volto-baseball",
  "version": "2.1.0",
  "description": "Volto wrapper around a Node.js Library for making requests to the MLB Stats API",
  "main": "src/index.js",
  "license": "MIT",
  "keywords": [
    "volto-addon",
    "volto",
    "plone",
    "react"
  ],
  "author": "Brian Davis",
  "homepage": "https://github.com/fosten/volto-baseball#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:fosten/volto-baseball.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "i18n": "rm -rf build/messages && NODE_ENV=production i18n --addon",
    "dry-release": "release-it --dry-run",
    "release": "release-it",
    "release-major-alpha": "release-it major --preRelease=alpha",
    "release-alpha": "release-it --preRelease=alpha"
  },
  "dependencies": {
    "axios": "*",
    "@asbeane/mlb-stats-api": "asbeane/mlb-stats-api#fd694b8cc32b82ba6fa8b9e024954ac4b85cb60b"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plone/scripts": "^3.6.1",
    "release-it": "^17.1.1"
  }
}
